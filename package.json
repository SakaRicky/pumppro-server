{
	"name": "e-store-pro-server",
	"version": "1.0.0",
	"description": "server for the e-store-pro build to manage store",
	"main": "index.js",
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"scripts": {
		"tsc": "tsc",
		"migrate:postgres": "dotenv -e .env -- npx prisma migrate dev --name postgres-init",
		"migrate:postgres_test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
		"dev": "cross-env NODE_ENV=development ts-node-dev --exit-child src/index.ts",
		"start": "ts-node src/index.ts",
		"lint": "eslint --ext .ts .",
		"test": "dotenv -e .env.test jest --verbose --runInBand",
		"build:ui": "rm -rf dist && cd ../client && npm run build && cp -r dist ../server"
	},
	"author": "Ricky Saka",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.18.6",
		"@flydotio/dockerfile": "^0.5.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.15",
		"@types/jest": "^29.2.6",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"babel-cli": "^6.26.0",
		"babel-jest": "^29.4.0",
		"babel-preset-env": "^1.7.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.32.0",
		"jest": "^29.3.1",
		"prisma": "^5.20.0",
		"superagent": "^8.0.8",
		"supertest": "^6.3.3",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"@prisma/client": "^5.20.0",
		"@types/bcrypt": "^5.0.0",
		"@types/express-session": "^1.17.5",
		"@types/jsonwebtoken": "^9.0.1",
		"@types/multer": "^1.4.7",
		"@types/supertest": "^2.0.12",
		"bcrypt": "^5.1.0",
		"body-parser": "^1.20.1",
		"cloudinary": "^1.41.1",
		"cors": "^2.8.5",
		"datauri": "^4.1.0",
		"dotenv": "^16.0.3",
		"dotenv-cli": "^7.0.0",
		"express": "^4.18.2",
		"express-async-handler": "^1.2.0",
		"express-session": "^1.17.3",
		"jsonwebtoken": "^9.0.0",
		"multer": "^1.4.5-lts.1",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"zod": "^3.20.2"
	}
}
